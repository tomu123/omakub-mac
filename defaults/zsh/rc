if type znap &>/dev/null; then
  znap source ~/.local/share/omakub-mac/defaults/zsh/shell
else
  source ~/.local/share/omakub-mac/defaults/zsh/shell
fi
znap source ~/.local/share/omakub-mac/defaults/zsh/aliases
znap source ~/.local/share/omakub-mac/defaults/zsh/functions
znap source ~/.local/share/omakub-mac/defaults/zsh/prompt
znap source ~/.local/share/omakub-mac/defaults/zsh/init
znap source ~/.local/share/omakub-mac/defaults/zsh/input